{
  "name": "@trustwallet/web3-provider-cosmos",
  "version": "4.0.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "private": true,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:types": "tsc",
    "build:source": "bun build:clean; rollup --config ./rollup.config.js; bun run build:types",
    "dev": "bun build ./index.ts --outdir ./dist --watch"
  },
  "dependencies": { "@cosmjs/amino": "0.32.3", "@cosmjs/proto-signing": "^0.32.3" },
  "devDependencies": {
    "@cosmos-kit/core": "^2.10.0",
    "@trustwallet/web3-provider-core": "workspace:*"
  }
}